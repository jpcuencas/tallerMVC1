@{
    ViewBag.Title = "Datos de nuestra oficina " + Model.nombre;
}

<h2>Datos de nuestra oficina @Model.nombre</h2>

<table>
<tr>
    <td>Código:</td>
    <td>@Model.id</td>
</tr>
<tr>
    <td>Nombre:</td>
    <td>@Model.nombre</td>
</tr>
<tr>
    <td>Dirección:</td>
    <td>@Model.direccion (@Model.poblacion)</td>
</tr>
<tr>
    <td>Teléfonos:</td>
    <td>@Model.telefonos</td>
</tr>

</table>
